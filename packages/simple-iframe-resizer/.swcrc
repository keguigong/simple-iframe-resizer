{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "es2022",
    "baseUrl": ".",
    "paths": {
      "@": ["./src/*"]
    }
  },
  "module": {
    "type": "nodenext",
    "strict": true,
    "noInterop": false
  },

  "sourceMaps": false,
  "exclude": [".*\\.test\\.(ts|tsx)$", ".*\\/test\\/*"]
}
